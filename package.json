  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/dist/index.js",
    "dev:server": "nodemon --exec ts-node server/src/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:server": "tsc -p server/tsconfig.json",
    "build:client": "cd client && npm run build",
    "build": "npm install --prefix client && npm run build:client && npm run build:server",
    "typecheck:server": "tsc -p server/tsconfig.json --noEmit",
    "typecheck:client": "cd client && tsc --noEmit",
    "typecheck": "npm run typecheck:server && npm run typecheck:client"
  },
